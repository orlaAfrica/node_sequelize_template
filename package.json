{
  "name": "computer-village",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/czarjulius/computer-village.git",
  "author": "czarjulius <juliusczar.jc@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "clear:build": "rimraf ./build",
    "build:watch": "tsc -w",
    "start": "node build/server.js",
    "start:dev": "rimraf ./build && tsc && nodemon build/server.js",
    "start:staging": "cross-env NODE_ENV=staging node build/server.js ",
    "dev": "concurrently -k \"tsc  -w\" \"nodemon build/server.js\"",
    "db:init": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "customer:migrate": "sequelize --options-path ./.sequelize-customer --env development db:migrate",
    "customer:seed": "sequelize --options-path ./.sequelize-customer --env development db:seed:all",
    "wallet:migrate": "sequelize --options-path ./.sequelize-wallet --env development db:migrate",
    "wallet:seed": "sequelize --options-path ./.sequelize-wallet --env development db:seed:all",
    "merchant:migrate": "sequelize --options-path ./.sequelize-merchant --env development db:migrate",
    "merchant:seed": "sequelize --options-path ./.sequelize-merchant --env development db:seed:all",
    "role:migrate": "sequelize --options-path ./.sequelize-role --env development db:migrate",
    "role:seed": "sequelize --options-path ./.sequelize-role --env development db:seed:all",
    "auth:migrate": "sequelize --options-path ./.sequelize-auth --env development db:migrate",
    "auth:seed": "sequelize --options-path ./.sequelize-auth --env development db:seed:all",
    "migration:clean": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "_moduleAliases": {
    "@config": "build/config",
    "@db": "src/db",
    "@helpers": "build/helpers",
    "@modules": "build/modules",
    "@middlewares": "build/middlewares",
    "@constants": "build/constants"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "helmet": "^6.0.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.5.1",
    "rimraf": "^3.0.2",
    "sequelize": "^6.25.8",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/sequelize": "^4.28.14",
    "@types/winston": "^2.4.4",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "typescript": "^4.9.3"
  }
}
